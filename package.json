{
  "name": "typer-obsidian",
  "version": "1.0.0",
  "description": "Fast & Minimal prefix-based suggestions in Obsidian editor",
  "main": "main.js",
  "scripts": {
    "prebuild": "bun scripts/init.js && tsc -noEmit -skipLibCheck",
    "build": "bun esbuild.config.mjs production",
    "config": "bun scripts/init.js",
    "version": "bun version-bump.mjs && git add manifest.json versions.json",
    "dev": "bun esbuild.config.mjs"
  },
  "keywords": [
    "plugin",
    "obsidian",
    "prefix",
    "suggestions",
    "autocomplete",
    "radix-trie"
  ],
  "author": "bastiangx",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "24.0.10",
    "@typescript-eslint/eslint-plugin": "8.35.1",
    "@typescript-eslint/parser": "8.35.1",
    "builtin-modules": "5.0.0",
    "esbuild": "0.25.5",
    "obsidian": "1.8.7",
    "tslib": "2.8.1",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.1.2",
    "@iarna/toml": "^3.0.0"
  }
}
