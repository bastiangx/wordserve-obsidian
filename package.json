{
  "name": "wordserve-obsidian",
  "version": "0.1.2",
  "description": "Fast & Minimal prefix-based Autosuggestions in Obsidian",
  "main": "main.js",
  "scripts": {
    "build": "bun esbuild.config.mjs production",
    "dev": "bun esbuild.config.mjs",
    "version": "bun version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "plugin",
    "obsidian",
    "prefix",
    "suggestions",
    "autocomplete",
    "radix-trie"
  ],
  "author": "bastiangx",
  "license": "MIT",
  "obsidianPlugin": {
    "name": "WordServe",
    "minAppVersion": "1.0.0",
    "description": "Autocomplete, suggestions & abbreviations | for devs & authors",
    "isDesktopOnly": true
  },
  "devDependencies": {
    "@types/node": "24.0.10",
    "@typescript-eslint/eslint-plugin": "8.35.1",
    "@typescript-eslint/parser": "8.35.1",
    "builtin-modules": "5.0.0",
    "esbuild": "0.25.5",
    "obsidian": "1.8.7",
    "tslib": "2.8.1",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.1.2",
    "@iarna/toml": "^3.0.0"
  }
}
