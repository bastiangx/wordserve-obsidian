{
	"name": "typr-obsidian",
	"version": "1.0.0",
	"description": "typr.obsidian plugin to suggest keywords for your notes -- (https://github.com/bastiangx/typr-obsidian)",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
    "prebuild": "node scripts/gen-config.js && tsc -noEmit -skipLibCheck",
		"build": "node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
    "config": "node scripts/gen-config.js"
	},
	"keywords": ["plugin", "obsidian", "keywords", "suggestions", "autocomplete"],
	"author": "bastiangx",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "22.15.18",
		"@typescript-eslint/eslint-plugin": "8.32.1",
		"@typescript-eslint/parser": "8.32.1",
		"obsidian": "latest",
		"builtin-modules": "5.0.0",
		"esbuild": "0.25.4",
		"tslib": "2.8.1",
		"typescript": "5.8.3"
	},
	"dependencies": {
		"@iarna/toml": "^3.0.0"
	}
}
